# IP Webcam Configuration
# Copy this file to .env and fill in your values

# Android IP address with port (e.g., 192.168.1.150:8080)
# Get this from the IP Webcam app when you start the server
ANDROID_IP=192.168.1.100:8080

# OBS WebSocket Configuration
OBS_HOST=localhost
OBS_PORT=4455
OBS_PASSWORD=your_password_here

# Video Configuration (optional)
VIDEO_WIDTH=1920
VIDEO_HEIGHT=1080
